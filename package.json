{
  "name": "cypress",
  "version": "1.0.0",
  "description": "UI testing",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress run",
    "cy:open:stage": "npx cypress open",
    "cy:open:prod": "npx cypress open --env userName=Santa,password=asdfgh,email=test-email2023@mail.ru --config baseUrl=https://santa-secret.ru/",
    "cy:dashboard:stage": "cypress run --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8",
    "cy:dashboard:prod": "npx cypress run --env userName=Santa,password=asdfgh,email=test-email2023@mail.ru --config baseUrl=https://santa-secret.ru/ --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8",
    "cy:run:chrome": "npx cypress run --browser chrome --spec 'cypress/e2e/secret-santa-links.cy.js' ",
    "cy:run:electron": "cypress run --browser electron --spec 'cypress/e2e/secret-santa.cy.js'",
    "cy:run:all": "npm run cy:run:chrome && npm run cy:run:electron",
    "cy:run:all:dashbord": "npm run cy:run:chrome -- --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8 && npm run cy:run:electron -- --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8",
    "cy:run:reg": "npx cypress run --browser chrome --spec 'cypress/e2e/registration.cy.js'",
    "cy:run:login": "npx cypress run --browser chrome --spec 'cypress/e2e/login.cy.js'",
    "cy:run:reg:login": "npm run cy:run:reg && npm run cy:run:login",
    "cy:run:test1": "npx cypress run --browser chrome --spec 'cypress/e2e/test1/*.cy.js' --config-file cypress1.config.js",
    "cy:run:test2": "npx cypress run --browser electron --spec 'cypress/e2e/test2/*.cy.js' --config-file cypress2.config.js",
    "cy:test1:dashboard": "npm run cy:run:test1 -- --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8",
    "cy:test2:dashboard": "npm run cy:run:test2 -- --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8",
    "cy:test1:test2:dashboard": "npm run cy:test1:dashboard && npm run cy:test2:dashboard",
    "cy:test1:test2:local:parallel": "npm run cy:run:test1 & npm run cy:run:test2",
    "cy:test1:test2:parallel": "npm run cy:test1:dashboard -- --group test1 --ci-build-id 1 --parallel & npm run cy:test2:dashboard -- --group test2 --ci-build-id 1 --parallel",
    "cy:test1:local:parallel": "cypress-parallel -s cy:run -t 2 -d cypress/e2e/test1/* -a '\"--config-file cypress1.config.js --browser chrome\"'",
    "cy:test2:local:parallel": "cypress-parallel -s cy:run -t 2 -d cypress/e2e/test2/* -a '\"--config-file cypress2.config.js --browser electron\"'",
    "cy:test1:parallel:dash": "cypress-parallel -s cy:run -t 2 -d cypress/e2e/test1/* -a '\"--config-file cypress1.config.js --browser chrome --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8\"'",
    "cy:test2:parallel:dash": "cypress-parallel -s cy:run -t 2 -d cypress/e2e/test2/* -a '\"--config-file cypress2.config.js --browser electron --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8\"'",
    "cy:test2:chrome": "npx cypress run --browser chrome --spec 'cypress/e2e/test2/*.cy.js' --config-file cypress2.config.js",
    "cy:test2:electron": "npx cypress run --browser electron --spec 'cypress/e2e/test2/*.cy.js' --config-file cypress2.config.js",
    "cy:spec1:spec2:parallel": " npx cypress run --browser chrome --spec 'cypress/e2e/test1/*' --config-file cypress1.config.js --record --key 3b4f35ff-ad5e-4d4b-acbe-1bfda2f8baf8 --group chrome --ci-build-id ${BUILD_ID} --parallel"
  },
  "author": "Natalia Siutsova <tashaklimenkova@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "cypress": "^12.7.0",
    "cypress-parallel": "^0.12.0"
  }
}
